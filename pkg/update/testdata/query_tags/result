query {

  r: repository(owner: "golang", name: "go") {
    nameWithOwner
    refs(refPrefix: "refs/tags/", query: "go1.19", orderBy: {field: TAG_COMMIT_DATE, direction: DESC}, first: 50) {
      totalCount
      nodes {
        name
        target {
          commitUrl
        }
      }
    }
  }, 

  r: repository(owner: "openjdk", name: "jdk11u") {
    nameWithOwner
    refs(refPrefix: "refs/tags/", query: "jdk-11", orderBy: {field: TAG_COMMIT_DATE, direction: DESC}, first: 50) {
      totalCount
      nodes {
        name
        target {
          commitUrl
        }
      }
    }
  }

}
